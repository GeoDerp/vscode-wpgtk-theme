	"tokenColors":
    [
		{
			"scope": ["function","class","method","meta.function.python","meta.function-call","heading", "punctuation.definition.list"],
			"settings": {
			"foreground": "#5B6254",
		}
		},
		{
			"scope": ["comment","string.quoted"],
			"settings": {
			"foreground": "#A5A59D70",
		}
		},
		{
			"scope": ["variable", "variable.legacy.builtin.python"],
			"settings": {
			"foreground": "#7E806E",
		}
		},
		{
			"scope": ["keyword.control", "support.type.python", "keyword.operator", "string.unquoted.argument"],
			"settings": {
			"foreground": "#C3C0B8",
		}
		},
		{
			"scope": ["meta.item-access.arguments","string.quoted.single"],
			"settings": {
			"foreground": "#e3e3e1",
		}
		},
	]
